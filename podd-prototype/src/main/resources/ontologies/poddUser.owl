<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY user "http://purl.org/podd/ns/poddUser#" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY poddModel "http://purl.org/podd/ns/podd#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://purl.org/podd/ns/poddUser#"
     xml:base="http://purl.org/podd/ns/poddUser"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:user="http://purl.org/podd/ns/poddUser#"
     xmlns:poddModel="http://purl.org/podd/ns/podd#">
    <owl:Ontology rdf:about="http://purl.org/podd/ns/poddUser">
        <owl:imports rdf:resource="http://purl.org/podd/ns/version/poddBase/1"/>
        <owl:versionIRI rdf:resource="http://purl.org/podd/ns/version/poddUser/1"/>
        <!-- Deactivate import of FOAF as it isn't needed in its entirety
            <owl:imports rdf:resource="http://xmlns.com/foaf/spec"/> 
        -->
        <rdfs:label>The PODD User Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/podd/ns/poddUser#hasAffiliation -->

    <owl:DatatypeProperty rdf:about="#hasAffiliation">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>



    <!-- http://purl.org/podd/ns/poddUser#hasPastTopObject -->

    <owl:ObjectProperty rdf:about="#hasPastTopObject">
        <rdfs:range rdf:resource="&poddModel;TopObject"/>
        <rdfs:domain rdf:resource="#User"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/podd/ns/poddUser#hasRepositoryRole -->

    <owl:ObjectProperty rdf:about="#hasRepositoryRole"/>
    


    <!-- http://purl.org/podd/ns/poddUser#onTopObject -->

    <owl:ObjectProperty rdf:about="#onTopObject">
        <rdfs:range rdf:resource="&poddModel;TopObject"/>
        <rdfs:domain rdf:resource="#TopObjectRoleBundle"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/podd/ns/poddUser#withRole -->

    <owl:ObjectProperty rdf:about="#withRole">
        <rdfs:domain rdf:resource="#TopObjectRoleBundle"/>
        <rdfs:range rdf:resource="#RoleRepositoryRole"/>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/mbox -->

    <owl:ObjectProperty rdf:about="&foaf;mbox"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/podd/ns/poddUser#hasAffiliation -->

    <owl:DatatypeProperty rdf:about="#hasAffiliation">
        <owl:dataPropertyRange rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/podd/ns/poddUser#lastLoginTime -->

    <owl:DatatypeProperty rdf:about="#lastLoginTime">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/podd/ns/poddUser#phoneNumber -->

    <owl:DatatypeProperty rdf:about="#phoneNumber">
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/podd/ns/poddUser#postalAddress -->

    <owl:DatatypeProperty rdf:about="#postalAddress">
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:DatatypeProperty rdf:about="&foaf;name"/>
    


    <!-- http://xmlns.com/foaf/0.1/title -->

    <owl:DatatypeProperty rdf:about="&foaf;title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/podd/ns/podd#TopObject -->

    <owl:Class rdf:about="&poddModel;TopObject"/>
    


    <!-- http://purl.org/podd/ns/poddUser#TopObjectRole -->

    <owl:Class rdf:about="#TopObjectRole">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#RoleMember"/>
                    <rdf:Description rdf:about="#RoleAdministrator"/>
                    <rdf:Description rdf:about="#RoleObserver"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://purl.org/podd/ns/poddUser#TopObjectRoleBundle -->

    <owl:Class rdf:about="#TopObjectRoleBundle">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#withRole"/>
                <owl:onClass rdf:resource="#TopObjectRole"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#onTopObject"/>
                <owl:onClass rdf:resource="&poddModel;TopObject"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A placeholder class to relate a user to his role in a particular top object. Through this class, a user has exactly one role on a particular top object.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/podd/ns/poddUser#RepositoryRole -->

    <owl:Class rdf:about="#RepositoryRole">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#RoleRepositoryAdministrator"/>
                    <rdf:Description rdf:about="#RoleStaffMember"/>
                    <rdf:Description rdf:about="#RolePublic"/>
                    <rdf:Description rdf:about="#RoleRepositoryUser"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment
            >Roles a user may have, such as administrator, curator, top object owner, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleRepositoryRole -->

    <owl:Class rdf:about="#RoleRepositoryRole"/>
    


    <!-- http://purl.org/podd/ns/poddUser#User -->

    <owl:Class rdf:about="#User">
        <rdfs:subClassOf rdf:resource="&foaf;Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#phoneNumber"/>
                <owl:allValuesFrom rdf:resource="&rdfs;Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#lastLoginTime"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRepositoryRole"/>
                <owl:onClass rdf:resource="#RepositoryRole"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#postalAddress"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&foaf;name"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRepositoryRole"/>
                <owl:allValuesFrom rdf:resource="#RepositoryRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&foaf;title"/>
                <owl:allValuesFrom rdf:resource="&rdfs;Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#lastLoginTime"/>
                <owl:allValuesFrom rdf:resource="&xsd;dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasAffiliation"/>
                <owl:someValuesFrom rdf:resource="&foaf;Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#phoneNumber"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&foaf;mbox"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A (human) user in the PODD system.

A user can have status either Active or Blocked.

A user has exactly one name (username).</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/podd/ns/poddUser#UserStatus -->

    <owl:Class rdf:about="#UserStatus">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#StatusActive"/>
                    <rdf:Description rdf:about="#StatusBlocked"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
    


    <!-- http://xmlns.com/foaf/0.1/Agent -->

    <owl:Class rdf:about="&foaf;Agent"/>
    


    <!-- http://xmlns.com/foaf/0.1/Organization -->

    <owl:Class rdf:about="&foaf;Organization"/>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="&foaf;Person"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/podd/ns/poddUser#StatusActive -->

    <UserStatus rdf:about="#StatusActive"/>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleTopObjectAdministrator -->

    <TopObjectRole rdf:about="#RoleTopObjectAdministrator">
        <rdfs:comment
            >Represents a leader of a top object.</rdfs:comment>
    </TopObjectRole>
    


    <!-- http://purl.org/podd/ns/poddUser#StatusBlocked -->

    <UserStatus rdf:about="#StatusBlocked"/>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleInformatician -->

    <RepositoryRole rdf:about="#RoleInformatician"/>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleMember -->

    <TopObjectRole rdf:about="#RoleMember">
        <rdfs:comment
            >Represents a member of a top object.</rdfs:comment>
    </TopObjectRole>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleObserver -->

    <TopObjectRole rdf:about="#RoleObserver"/>
    


    <!-- http://purl.org/podd/ns/poddUser#RolePublic -->

    <RepositoryRole rdf:about="#RolePublic">
        <rdfs:comment
            >Represents a public/anonymous user.</rdfs:comment>
    </RepositoryRole>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleRepositoryAdministrator -->

    <RepositoryRole rdf:about="#RoleRepositoryAdministrator">
        <rdfs:comment
            >Reresents a user with administrator privilidges.</rdfs:comment>
    </RepositoryRole>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleRepositoryUser -->

    <RepositoryRole rdf:about="#RoleRepositoryUser"/>
    


    <!-- http://purl.org/podd/ns/poddUser#RoleStaffMember -->

    <RepositoryRole rdf:about="#RoleStaffMember">
        <rdfs:comment
            >Represents a regular registered user</rdfs:comment>
    </RepositoryRole>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDifferent"/>
        <owl:distinctMembers rdf:parseType="Collection">
            <rdf:Description rdf:about="#RoleStaffMember"/>
            <rdf:Description rdf:about="#RolePublic"/>
        </owl:distinctMembers>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDifferent"/>
        <owl:distinctMembers rdf:parseType="Collection">
            <rdf:Description rdf:about="#StatusActive"/>
            <rdf:Description rdf:about="#StatusBlocked"/>
        </owl:distinctMembers>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDifferent"/>
        <owl:distinctMembers rdf:parseType="Collection">
            <rdf:Description rdf:about="#RoleRepositoryAdministrator"/>
            <rdf:Description rdf:about="#RolePublic"/>
        </owl:distinctMembers>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDifferent"/>
        <owl:distinctMembers rdf:parseType="Collection">
            <rdf:Description rdf:about="#RoleMember"/>
            <rdf:Description rdf:about="#RoleObserver"/>
        </owl:distinctMembers>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDifferent"/>
        <owl:distinctMembers rdf:parseType="Collection">
            <rdf:Description rdf:about="#RoleTopObjectAdministrator"/>
            <rdf:Description rdf:about="#RoleObserver"/>
        </owl:distinctMembers>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDifferent"/>
        <owl:distinctMembers rdf:parseType="Collection">
            <rdf:Description rdf:about="#RoleMember"/>
            <rdf:Description rdf:about="#RoleTopObjectAdministrator"/>
        </owl:distinctMembers>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDifferent"/>
        <owl:distinctMembers rdf:parseType="Collection">
            <rdf:Description rdf:about="#RoleRepositoryAdministrator"/>
            <rdf:Description rdf:about="#RoleStaffMember"/>
        </owl:distinctMembers>
    </rdf:Description>
</rdf:RDF>




