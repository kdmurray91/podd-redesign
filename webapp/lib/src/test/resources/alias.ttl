@prefix fileRepository: <http://purl.org/podd/ns/fileRepository#> .
@prefix poddBase: <http://purl.org/podd/ns/poddBase#> .
@prefix : <http://test.example.org/filerepositoryaliases/> .

# Default File Repositories for use with PODD unit tests.
# This file is used ONLY if the file-repository-management graph of the RDF database does not have any configured
# file repositories.

# alias details for a default SSH based file repository
:localssh a fileRepository:FileRepository , fileRepository:SSHFileRepository ;
poddBase:hasAlias "alias_local_ssh" ;
fileRepository:hasFileRepositoryProtocol "SSH" ;
fileRepository:hasFileRepositoryHost "localhost" ;
fileRepository:hasFileRepositoryPort "9856" ;
fileRepository:hasFileRepositoryFingerprint "ce:a7:c1:cf:17:3f:96:49:6a:53:1a:05:0b:ba:90:db" ;
fileRepository:hasFileRepositoryUsername "salt" ;
fileRepository:hasFileRepositorySecret "salt" .
